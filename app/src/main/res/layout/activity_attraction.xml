<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.tourguide.AttractionActivity">

    <Button
        android:id="@+id/firstAttractionButton_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="calicut beach" />

    <Button
        android:id="@+id/secondAttractionButton_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Thusharagiri falls" />

    <Button
        android:id="@+id/thirdAttractionButton_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="planetarium" />

    <Button
        android:id="@+id/fourthAttractionButton_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="peruvannamoozhi dam" />
</LinearLayout>
