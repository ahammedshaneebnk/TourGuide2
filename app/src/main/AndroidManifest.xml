<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IntroActivity"
            android:label="Introduction"></activity>
        <activity
            android:name=".AttractionActivity"
            android:label="Attractions"></activity>
        <activity
            android:name=".RestaurantActivity"
            android:label="Restaurants"></activity>
        <activity
            android:name=".TransportationActivity"
            android:label="Transportation"></activity>
        <activity
            android:name=".BeachActivity"
            android:label="Calicut Beach"></activity>
        <activity android:name=".ThusharagiriActivity"></activity>
    </application>

</manifest>